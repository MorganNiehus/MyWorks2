
public class Main {
	
	public static void main(String[] args){
			
	
		System.out.println(swapString("VALDOSTA"));
		
	}
	public static String swapString(String word){
		
		int mid = word.length() / 2;
		
		String end = word.substring(mid);
		String begin = word.substring(0 , mid);
		
		return end + " " + begin;
		
	}

}
